{
    "name": "@acme/ui",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "exports": {
        "./util": "./src/util.ts",
        "./components/*": "./src/components/*.tsx",
        "./icons/*": "./src/icons/*.tsx"
    },
    "scripts": {
        "clean": "rm -rf .turbo node_modules",
        "format": "prettier --check . --ignore-path ../../../.prettierignore",
        "jest": "pnpm run -C ../../.. jest",
        "jest:i": "RUN_JEST_INTEGRATION_TESTS=1 pnpm run jest",
        "lint": "eslint .",
        "shad": "pnpm dlx shadcn-ui@latest add",
        "postshad": "pnpm run format --write && pnpm run lint --fix",
        "test": "pnpm run jest $PWD",
        "test:i": "pnpm run jest:i $PWD",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@andrao/tailwind": "1.2.0",
        "@hookform/resolvers": "3.3.4",
        "@radix-ui/react-avatar": "1.1.0",
        "@radix-ui/react-dialog": "1.1.1",
        "@radix-ui/react-dropdown-menu": "2.1.1",
        "@radix-ui/react-hover-card": "1.1.1",
        "@radix-ui/react-icons": "1.3.0",
        "@radix-ui/react-label": "2.0.2",
        "@radix-ui/react-popover": "1.0.7",
        "@radix-ui/react-progress": "1.0.3",
        "@radix-ui/react-select": "2.0.0",
        "@radix-ui/react-separator": "1.0.3",
        "@radix-ui/react-slider": "1.1.2",
        "@radix-ui/react-slot": "1.0.2",
        "@radix-ui/react-switch": "1.0.3",
        "@radix-ui/react-tabs": "1.0.4",
        "@radix-ui/react-toggle": "1.0.3",
        "@radix-ui/react-toggle-group": "1.0.4",
        "@radix-ui/react-tooltip": "1.0.7",
        "class-variance-authority": "0.7.0",
        "cmdk": "1.0.0",
        "framer-motion": "11.3.21",
        "lucide-react": "0.364.0",
        "next-themes": "0.3.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "7.51.2",
        "sonner": "1.4.41",
        "vaul": "0.9.0",
        "zod": "3.23.8"
    }
}
