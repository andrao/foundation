{
    "extends": "@andrao/tsconfig/base.json",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "/*": ["./public/*"],
            "~/*": ["./src/*"],
            "@ui/*": ["../../packages/core/browser/ui/src/*"]
        },
        "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
    },
    "include": ["src", "src/env.js", "vite.config.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
