{
    "extends": "@andrao/tsconfig/base.json",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": ["./src/*"],
            "@ui/*": ["../../packages/@app-browser/ui/src/*"]
        },
        "plugins": [{ "name": "next" }],
        "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
    },
    "include": [
        "next-env.d.ts",
        "src/env.js",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "../../packages/@app-browser/ui/src/components/molecules/KeyboardInput.tsx"
    ],
    "exclude": ["node_modules"]
}
